
<% if (draftMode === "visible" || item.draft !== true) {%>
  <li class="nav-item<%- item.text === undefined ? ' compact' : '' %>">
    <% if (item.href) { %>
    <a class="nav-link" href="<%- item.href %>"<%= item.rel ? ` rel="${item.rel}"` : "" %><%= item.target ? ` target="${item.target}"` : "" %>>
    <% } %>
      <% partial('navicon.ejs', { item }) %> <%= item.text %>
    <% if (item.href) { %>
    </a>
    <% } %>
  </li>  
<% } %>
